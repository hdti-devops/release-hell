{
  "separate-pull-requests": true,
  "sequential-calls": true,
  "skip-github-release": false,
  "packages": {
    "charts/argocd": {
        "release-type": "helm",
        "package-name": "argocd"
    },
    ".": {
      "release-type": "simple",
      "package-name": "nginx-oci",
      "version-file": "charts/nginx-oci/version.txt",
      "include-paths": [
        "charts/nginx-oci/**",
        "oci-base/**/nginx-oci",
        "oci-base/**/nginx-oci/**",
        "clusters/**/nginx-oci",
        "clusters/**/nginx-oci/**"
      ]
    },
    "charts/oci-new": {
      "release-type": "simple",
      "package-name": "oci-new",
      "version-file": "nginx-oci/version.txt",
      "include-paths": [
        "charts/oci-new",
        "charts/oci-new/**",
        "oci-base/**/oci-new",
        "../oci-base/**/oci-new",
        "../../oci-base/**/oci-new",
        "oci-base/**/oci-new/kustomization.yaml",
        "../oci-base/**/oci-new/kustomization.yaml",
        "../../oci-base/**/oci-new/kustomization.yaml",
        "../../oci-base/**/oci-new/**"
      ],
      "extra-files": [
        "charts/oci-new/version.txt"
      ]
    },
    "charts/occ": {
      "release-type": "simple",
      "package-name": "occ",
      "version-file": "version.txt",
      "include-paths": [
        "charts/occ",
        "charts/occ/**",
        "oci-base/**/occ",
        "../oci-base/**/occ",
        "../../oci-base/**/occ",
        "oci-base/**/occ/kustomization.yaml",
        "../oci-base/**/occ/kustomization.yaml",
        "../../oci-base/**/occ/kustomization.yaml",
        "../../oci-base/**/occ/**"
      ],
      "extra-files": [
        "charts/occ/version.txt"
      ]
    },
    "ocout": {
      "release-type": "simple",
      "package-name": "nginx-oci",
      "version-file": "version.txt",
      "include-paths": [
        "oci-base/**/ocout/**",
        "clusters/**/ocout/**",
        "ocout/**"
      ]
    }
  }
}