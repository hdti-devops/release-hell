{
  "separate-pull-requests": true,
  "sequential-calls": true,
  "skip-github-release": false,
  "packages": {
    "charts/argocd": {
        "release-type": "helm",
        "package-name": "argocd"
    },
    ".": {
      "release-type": "simple",
      "package-name": "nginx-oci",
      "version-file": "charts/nginx-oci/version.txt",
      "include-paths": [
        "charts/nginx-oci/**",
        "oci-base/**/nginx-oci",
        "oci-base/**/nginx-oci/**",
        "clusters/**/nginx-oci",
        "clusters/**/nginx-oci/**"
      ]
    },
    "charts/oci-new": {
      "release-type": "simple",
      "package-name": "oci-new",
      "version-file": "version.txt",
      "include-paths": [
        "charts/oci-new/**",
        "../../oci-base/**/oci-new",
        "../../oci-base/**/oci-new/**"
      ]
    }
  }
}